{
  "expo": {
    "name": "app-med",
    "slug": "app-med",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/img/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/img/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./src/img/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
